 {
  "Lua.workspace.maxPreload": 2000,
  "Lua.workspace.preloadFileSize": 10000,
  "suggest.removeDuplicateItems": true,
  "coc.preferences.jumpCommand": "tabe",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualText": true,
  "codeLens.enable": true,
  "suggest.floatEnable": true,
  "suggest.detailField": "preview",
  "list.source.symbols.excludes": ["node_modules"],
  "suggest.enablePreview": true,
  "explorer.icon.enableVimDevicons": true,
  "git.changedSign.text": "",
  "yaml.schemas": {
    "http://json.schemastore.org/github-workflow": "*/.github/workfows/*.yaml"
  },
  "git.addedSign.text": "",
  "git.removedSign.text": "",
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.infoSign": "",
  "suggest.timeout": 5000,
  "suggest.triggerCompletionWait": 100,
  "tsserver.log": "verbose",
  "diagnostic.hintSign": "",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript.jasmine",
    "sh",
    "vim",
    "markdown",
    "Markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "python",
    "typescriptreact",
    "swift",
    "html",
    "css",
    "scss",
    "json",
    "yaml"
  ],

  "python.jediEnabled": false,
  "languageserver": {
    "swift": {
      "command": "sourcekit-lsp",
      "args": [],
      "filetypes": ["swift"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
